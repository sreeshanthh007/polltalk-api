{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "rootDir": "./src",
    "outDir": "./dist",
    
    "baseUrl": "./src",
    
    "paths": {
      "@controllers/*":["controllers/*"],
      "@socket/*":["socket/*"],
      "@socketEvents/*":["socketEvents/*"]
    },
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true
  }
}
