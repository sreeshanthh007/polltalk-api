{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "rootDir": "./src",
    "outDir": "./dist",
    
    "baseUrl": "./src",
    
    "paths": {
      "@controllers/*":["controllers/*"],
      "@socket/*":["socket/*"],
      "@websocket/*":["websocket/*"],
      "@socketEvents/*": ["socket/events/*"],
      "@handlers/*":["websocket/handlers/*"],
      "@models/*":["models/*"],
      "@constants/*":["shared/*"]
    },
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true
  }
}
